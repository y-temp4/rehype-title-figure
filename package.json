{
  "name": "rehype-title-figure",
  "version": "0.1.2",
  "description": "Rehype plugin that adds a caption using the title attribute of the img.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "clean": "rimraf lib",
    "format": "prettier {src,tests}/**/*.ts -w",
    "release": "standard-version",
    "postinstall": "husky install",
    "prepublishOnly": "yarn clean && yarn build && pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y-temp4/rehype-title-figure.git"
  },
  "homepage": "https://github.com/y-temp4/rehype-title-figure#readme",
  "author": "y-temp4 <y.temp4@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/y-temp4/rehype-title-figure/issues"
  },
  "keywords": [
    "remark",
    "rehype",
    "rehype-plugin"
  ],
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@types/hast": "2.3.1",
    "@types/jest": "26.0.23",
    "hast": "1.0.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "rehype-raw": "5.1.0",
    "rehype-stringify": "8.0.0",
    "remark-parse": "9.0.0",
    "remark-rehype": "8.1.0",
    "rimraf": "3.0.2",
    "standard-version": "9.2.0",
    "to-vfile": "6.1.0",
    "ts-jest": "26.5.5",
    "typescript": "4.2.4",
    "unified": "9.2.1"
  },
  "dependencies": {
    "hastscript": "6.0.0",
    "unist-util-visit": "2.0.3"
  }
